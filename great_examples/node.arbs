صنف العقدة {
    منشئ(البيانات، تالي){
        البيانات هذا = البيانات؛
        تالي هذا = تالي؛
    }

    دالة تعيين&تالي(تالي){
        تالي هذا = تالي؛
    }
    دالة تعيين&البيانات(البيانات){
        البيانات هذا = البيانات؛
    }

    دالة احصل&تالي(){
        عد تالي هذا؛
    }
    دالة احصل&البيانات(){
        عد البيانات هذا؛
    }
}